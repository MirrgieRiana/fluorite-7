#!/usr/bin/env bash

usage() {
	echo "Usage $0 CODE" >&2
	exit 1
}

(($# > 0)) || usage
exec=$1
shift

(($# > 0)) && usage

#

cd "$(cd "$(dirname "$0")"; pwd)"

if [ ! -f ./fluorite-7.js ]
then
	./compile.bash || exit 2
fi

node -e 'var a = require("./fluorite-7.js"); console.log(a.parse(process.argv[1])[1])' "$exec"
